                      </object>
                    </child>
                    <child>
                      <object class="AdwViewStackPage">
                        <property name="name">backup</property>
                        <property name="title">Backup</property>
                        <property name="icon-name">info-backup-symbolic</property>
                        <property name="child">
                          <object class="GtkBox" id="backup_box">
