                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkButton" id="log_copy_button">
                                        <property name="icon-name">edit-copy-symbolic</property>
                                        <property name="tooltip-text">Copy Log Messages</property>
                                        <style>
                                          <class name="flat"/>
                                          <class name="circular"/>
                                        </style>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkOverlay">
                                <child>
                                  <object class="GtkScrolledWindow">
                                    <style>
                                      <class name="card"/>
                                    </style>
                                    <property name="hexpand">true</property>
                                    <property name="vexpand">true</property>
                                    <property name="margin-top">6</property>
                                    <property name="margin-bottom">6</property>
                                    <property name="margin-start">24</property>
                                    <property name="margin-end">24</property>
                                    <property name="child">
                                      <object class="GtkColumnView" id="log_view">
                                        <style>
                                          <class name="card-list"/>
                                        </style>
                                        <property name="margin-bottom">16</property>
                                        <property name="margin-start">12</property>
                                        <property name="margin-end">12</property>
                                        <property name="tab-behavior">item</property>
                                        <property name="model">
                                          <object class="GtkNoSelection" id="log_selection">
                                            <property name="model">
                                              <object class="GListStore" id="log_model">
                                                <property name="item-type">GtkStringObject</property>
                                              </object>
                                            </property>
                                          </object>
                                        </property>
                                        <child>
                                          <object class="GtkColumnViewColumn">
                                            <property name="title">Log Message</property>
                                            <property name="expand">true</property>
                                            <property name="factory">
                                              <object class="GtkBuilderListItemFactory">
                                                <property name="resource">/com/github/PacView/ui/info_pane/log_item.ui</property>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                                <child type="overlay">
                                  <object class="AdwStatusPage" id="log_paused_status">
                                    <property name="visible">false</property>
                                    <property name="title">View Update Paused</property>
                                    <property name="paintable">
                                      <object class="AdwSpinnerPaintable">
                                        <property name="widget">log_paused_status</property>
                                      </object>
                                    </property>
                                    <style>
                                      <class name="compact"/>
                                      <class name="dimmed"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwViewStackPage">
                        <property name="name">cache</property>
