                    </child>
                    <child>
                      <object class="AdwActionRow" id="totaldownload_row">
                        <property name="title">TotalDownload</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="disabledownloadtimeout_row">
                        <property name="title">DisableDownloadTimeout</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="ilovecandy_row">
                        <property name="title">ILoveCandy</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup">
                    <child>
                      <object class="AdwActionRow" id="siglevel_row">
                        <property name="title">SigLevel</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="localfilesiglevel_row">
                        <property name="title">LocalFileSigLevel</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="remotefilesiglevel_row">
                        <property name="title">RemoteFileSigLevel</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </property>
      </object>
    </property>
  </template>
</interface>
