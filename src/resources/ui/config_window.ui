<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// ConfigWindow template //-->
  <template class="ConfigWindow" parent="AdwWindow">
    <property name="default-width">500</property>
    <property name="default-height">500</property>
    <property name="modal">True</property>
    <property name="title"></property>
    <property name="content">
      <object class="AdwToolbarView" id="toolbar_view">
        <child type="top">
          <object class="AdwHeaderBar" id="header_bar">
          </object>
        </child>
        <property name="content">
          <object class="GtkScrolledWindow">
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <property name="child">
              <object class="GtkBox">
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">12</property>
                <property name="margin-bottom">24</property>
                <property name="spacing">30</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="AdwPreferencesGroup">
                    <child>
                      <object class="AdwActionRow" id="rootdir_row">
                        <property name="title">RootDir</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="rootdir_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="dbpath_row">
                        <property name="title">DBPath</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="dbpath_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="cachedir_row">
                        <property name="title">CacheDir</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="cachedir_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="logfile_row">
                        <property name="title">LogFile</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="logfile_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="gpgdir_row">
                        <property name="title">GPGDir</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="gpgdir_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="hookdir_row">
                        <property name="title">HookDir</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                        <child>
                          <object class="GtkButton" id="hookdir_open_button">
                            <property name="valign">center</property>
                            <property name="icon-name">go-next-symbolic</property>
                            <property name="tooltip-text">Show in File Manager</property>
                            <style>
                              <class name="flat"/>
                              <class name="circular"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="xfercommand_row">
                        <property name="title">XferCommand</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="cleanmethod_row">
                        <property name="title">CleanMethod</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="architecture_row">
                        <property name="title">Architecture</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup">
                    <child>
                      <object class="AdwActionRow" id="holdpkg_row">
                        <property name="title">HoldPkg</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="ignorepkg_row">
                        <property name="title">IgnorePkg</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="ignoregroup_row">
                        <property name="title">IgnoreGroup</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="noupgrade_row">
                        <property name="title">NoUpgrade</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
                          <class name="property"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow" id="noextract_row">
                        <property name="title">NoExtract</property>
                        <property name="subtitle-selectable">True</property>
                        <style>
