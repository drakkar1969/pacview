<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// DetailsWindow template //-->
  <template class="DetailsWindow" parent="AdwApplicationWindow">
    <property name="default-width">650</property>
    <property name="default-height">650</property>
    <property name="modal">True</property>
    <property name="title"></property>
    <property name="content">
      <object class="AdwToolbarView" id="toolbar_view">
        <child type="top">
          <object class="AdwHeaderBar" id="header_bar">
          </object>
        </child>
        <property name="content">
          <object class="GtkBox" id="main_box">
            <property name="orientation">vertical</property>
            <property name="margin-start">24</property>
            <property name="margin-end">24</property>
            <property name="margin-bottom">24</property>
            <property name="spacing">16</property>
            <child>
              <object class="GtkImage" id="pkg_image">
                <property name="halign">center</property>
                <property name="icon-name">package-x-generic-symbolic</property>
                <property name="icon-size">large</property>
                <style>
                  <class name="bubble"/>
                  <class name="large"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="pkg_label">
                <property name="margin-start">16</property>
                <property name="margin-end">16</property>
                <property name="margin-bottom">24</property>
                <style>
                  <class name="title-2"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="button_box">
                <property name="homogeneous">True</property>
                <property name="spacing">6</property>
                <child>
                  <object class="ToggleButton" id="files_button">
                    <property name="hexpand">True</property>
                    <property name="active">True</property>
                    <property name="icon">details-files-symbolic</property>
                    <property name="text">Files</property>
                    <property name="action-name">win.set-tab</property>
                    <property name="action-target">"files"</property>
                  </object>
                </child>
                <child>
                  <object class="ToggleButton" id="log_button">
                    <property name="hexpand">True</property>
                    <property name="group">files_button</property>
                    <property name="icon">details-log-symbolic</property>
                    <property name="text">Log</property>
                    <property name="action-name">win.set-tab</property>
                    <property name="action-target">"log"</property>
                  </object>
                </child>
                <child>
                  <object class="ToggleButton" id="cache_button">
                    <property name="hexpand">True</property>
                    <property name="group">files_button</property>
                    <property name="icon">details-cache-symbolic</property>
                    <property name="text">Cache</property>
                    <property name="action-name">win.set-tab</property>
                    <property name="action-target">"cache"</property>
                  </object>
                </child>
                <child>
                  <object class="ToggleButton" id="backup_button">
                    <property name="hexpand">True</property>
                    <property name="group">files_button</property>
                    <property name="icon">details-backup-symbolic</property>
                    <property name="text">Backup</property>
                    <property name="action-name">win.set-tab</property>
                    <property name="action-target">"backup"</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkStack" id="content_stack">
                <style>
                  <class name="card"/>
                </style>
                <child>
                  <object class="GtkStackPage" id="none_page">
                    <property name="name">none</property>
                    <property name="child">
                      <object class="GtkLabel">
                        <property name="label">No information available</property>
                        <style>
                          <class name="title-4"/>
                          <class name="dim-label"/>
                        </style>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage" id="files_page">
                    <property name="name">files</property>
                    <property name="child">
                      <object class="GtkBox" id="files_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <child>
                          <object class="GtkBox" id="files_header_box">
                            <property name="margin-start">16</property>
                            <property name="margin-end">16</property>
                            <property name="margin-top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage" id="files_header_image">
                                <property name="icon-name">details-files-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="files_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Files</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <property name="hexpand">True</property>
                                <property name="halign">end</property>
                                <property name="margin-start">20</property>
                                <property name="spacing">20</property>
                                <child>
                                  <object class="GtkSearchEntry" id="files_search_entry">
                                    <property name="tooltip-text">Search for Files</property>
                                    <style>
                                      <class name="rounded"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkBox">
                                    <property name="spacing">6</property>
                                    <child>
                                      <object class="GtkButton" id="files_open_button">
                                        <property name="icon-name">folder-open-symbolic</property>
                                        <property name="tooltip-text">Show Selected in File Manager</property>
                                        <style>
                                          <class name="flat"/>
                                          <class name="circular"/>
                                        </style>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkButton" id="files_copy_button">
                                        <property name="icon-name">edit-copy-symbolic</property>
                                        <property name="tooltip-text">Copy File List</property>
                                        <style>
                                          <class name="flat"/>
                                          <class name="circular"/>
                                        </style>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin-bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="files_view">
                                <property name="margin-start">16</property>
                                <property name="margin-end">16</property>
                                <property name="tab-behavior">item</property>
                                <property name="model">
                                  <object class="GtkSingleSelection" id="files_selection">
                                    <property name="model">
                                      <object class="GtkFilterListModel" id="files_filter_model">
                                        <property name="filter">
                                          <object class="GtkStringFilter" id="files_filter">
                                            <property name="expression">
                                              <lookup name="string" type="GtkStringObject"/>
                                            </property>
                                          </object>
                                        </property>
                                        <property name="model">
                                          <object class="GListStore" id="files_model">
                                            <property name="item-type">GtkStringObject</property>
                                          </object>
                                        </property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory">
                                    <property name="resource">/com/github/PacView/ui/details_window/file_item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage" id="log_page">
                    <property name="name">log</property>
                    <property name="child">
                      <object class="GtkBox" id="log_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <child>
                          <object class="GtkBox" id="log_header_box">
                            <property name="margin-start">16</property>
                            <property name="margin-end">16</property>
                            <property name="margin-top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage" id="log_header_image">
                                <property name="icon-name">details-log-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="log_header_label">
                                <property name="hexpand">True</property>
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Log Messages</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton" id="log_copy_button">
                                <property name="icon-name">edit-copy-symbolic</property>
                                <property name="tooltip-text">Copy Log Messages</property>
                                <style>
                                  <class name="flat"/>
                                  <class name="circular"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin-bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="log_view">
                                <property name="margin-start">16</property>
                                <property name="margin-end">16</property>
                                <property name="tab-behavior">item</property>
                                <property name="model">
                                  <object class="GtkNoSelection" id="log_selection">
                                    <property name="model">
                                      <object class="GListStore" id="log_model">
                                        <property name="item-type">GtkStringObject</property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory">
                                    <property name="resource">/com/github/PacView/ui/details_window/log_item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage" id="cache_page">
                    <property name="name">cache</property>
                    <property name="child">
                      <object class="GtkBox" id="cache_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <child>
                          <object class="GtkBox" id="cache_header_box">
                            <property name="margin-start">16</property>
                            <property name="margin-end">16</property>
                            <property name="margin-top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage" id="cache_header_image">
                                <property name="icon-name">details-cache-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="cache_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Cache</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <property name="hexpand">True</property>
                                <property name="halign">end</property>
                                <property name="spacing">6</property>
                                <child>
                                  <object class="GtkButton" id="cache_open_button">
                                    <property name="icon-name">folder-open-symbolic</property>
                                    <property name="tooltip-text">Show Selected in File Manager</property>
                                    <style>
                                      <class name="flat"/>
                                      <class name="circular"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="cache_copy_button">
                                    <property name="icon-name">edit-copy-symbolic</property>
                                    <property name="tooltip-text">Copy Cache File List</property>
                                    <style>
                                      <class name="flat"/>
                                      <class name="circular"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin-bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="cache_view">
                                <property name="margin-start">16</property>
                                <property name="margin-end">16</property>
                                <property name="tab-behavior">item</property>
                                <property name="model">
                                  <object class="GtkSingleSelection" id="cache_selection">
                                    <property name="model">
                                      <object class="GListStore" id="cache_model">
                                        <property name="item-type">GtkStringObject</property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory">
                                    <property name="resource">/com/github/PacView/ui/details_window/cache_item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage" id="backup_page">
                    <property name="name">backup</property>
                    <property name="child">
                      <object class="GtkBox" id="backup_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <child>
                          <object class="GtkBox" id="backup_header_box">
                            <property name="margin-start">16</property>
                            <property name="margin-end">16</property>
                            <property name="margin-top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage" id="backup_header_image">
                                <property name="icon-name">details-backup-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="backup_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Backup</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <property name="hexpand">True</property>
                                <property name="halign">end</property>
                                <property name="spacing">6</property>
                                <child>
                                  <object class="GtkButton" id="backup_open_button">
                                    <property name="icon-name">folder-open-symbolic</property>
                                    <property name="tooltip-text">Show Selected in File Manager</property>
                                    <style>
                                      <class name="flat"/>
                                      <class name="circular"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="backup_copy_button">
                                    <property name="icon-name">edit-copy-symbolic</property>
                                    <property name="tooltip-text">Copy Backup File List</property>
                                    <style>
                                      <class name="flat"/>
                                      <class name="circular"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin-bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="backup_view">
                                <property name="margin-start">16</property>
                                <property name="margin-end">16</property>
                                <property name="tab-behavior">item</property>
                                <property name="model">
                                  <object class="GtkSingleSelection" id="backup_selection">
                                    <property name="model">
                                      <object class="GListStore" id="backup_model">
                                        <property name="item-type">BackupObject</property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory">
                                    <property name="resource">/com/github/PacView/ui/details_window/backup_item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
      </object>
    </property>
  </template>
</interface>
