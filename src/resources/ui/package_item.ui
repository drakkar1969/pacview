<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// PackageItem template //-->
  <template class="PackageItem" parent="GtkBox">
    <property name="margin-start">12</property>
    <property name="margin-end">12</property>
    <property name="margin-top">6</property>
    <property name="margin-bottom">8</property>
    <property name="spacing">12</property>
    <child>
      <object class="GtkImage">
        <property name="icon-name">package-x-generic-symbolic</property>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">5</property>
        <child>
          <object class="GtkBox">
            <property name="spacing">48</property>
            <child>
              <object class="GtkLabel" id="name_label">
                <property name="valign">baseline-center</property>
                <property name="hexpand">true</property>
                <property name="xalign">0</property>
                <property name="ellipsize">end</property>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="valign">baseline-center</property>
                <property name="spacing">6</property>
                <child>
                  <object class="GtkImage" id="version_image">
                    <property name="icon-name">pkg-update</property>
                    <property name="valign">center</property>
                    <style>
                      <class name="status-image"/>
                      <class name="success"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="version_label">
                    <property name="valign">baseline-center</property>
                    <property name="xalign">0</property>
                    <style>
                      <class name="caption"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="spacing">48</property>
            <child>
              <object class="GtkBox">
                <property name="spacing">6</property>
                <child>
                  <object class="GtkImage" id="status_image">
                    <property name="margin-top">1</property>
                    <property name="halign">start</property>
                    <style>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="status_label">
                    <property name="xalign">0</property>
                    <style>
                      <class name="caption"/>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator"/>
                </child>
                <child>
                  <object class="GtkLabel" id="repository_label">
                    <property name="xalign">0</property>
                    <style>
                      <class name="caption"/>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator"/>
                </child>
                <child>
                  <object class="GtkLabel" id="size_label">
                    <property name="hexpand">true</property>
                    <property name="xalign">0</property>
                    <style>
                      <class name="caption"/>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="spacing">6</property>
                <child>
                  <object class="GtkImage" id="groups_image">
                    <property name="margin-top">1</property>
                    <property name="icon-name">groups-symbolic</property>
                    <style>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="groups_label">
                    <property name="xalign">0</property>
                    <property name="ellipsize">end</property>
                    <style>
                      <class name="caption"/>
                      <class name="dimmed"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
