<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// ConfigDialog template //-->
  <template class="ConfigDialog" parent="AdwDialog">
    <property name="content-width">550</property>
    <property name="content-height">600</property>
    <property name="focus-widget">rootdir_row</property>
    <property name="title">Pacman Config</property>
    <child>
      <object class="AdwToolbarView" id="toolbar_view">
        <child type="top">
          <object class="AdwHeaderBar" id="header_bar">
            <child>
              <object class="GtkButton" id="config_button">
                <property name="label">_Open</property>
                <property name="tooltip-text">Open Pacman Config</property>
                <property name="use-underline">true</property>
                <style>
                  <class name="flat"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <property name="content">
          <object class="AdwPreferencesPage">
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">Paths</property>
                <child>
                  <object class="AdwActionRow" id="rootdir_row">
                    <property name="title">RootDir</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="rootdir_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="dbpath_row">
                    <property name="title">DBPath</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="dbpath_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="cachedir_row">
                    <property name="title">CacheDir</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="cachedir_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="logfile_row">
                    <property name="title">LogFile</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="logfile_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="gpgdir_row">
                    <property name="title">GPGDir</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="gpgdir_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="hookdir_row">
                    <property name="title">HookDir</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                    <child>
                      <object class="GtkButton" id="hookdir_open_button">
                        <property name="valign">center</property>
                        <property name="icon-name">go-next-symbolic</property>
                        <property name="tooltip-text">Open</property>
                        <style>
                          <class name="flat"/>
                          <class name="circular"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">Download</property>
                <child>
                  <object class="AdwActionRow" id="xfercommand_row">
                    <property name="title">XferCommand</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="paralleldownloads_row">
                    <property name="title">ParallelDownloads</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="disabledownloadtimeout_row">
                    <property name="title">DisableDownloadTimeout</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="downloaduser_row">
                    <property name="title">DownloadUser</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="architecture_row">
                    <property name="title">Architecture</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">SandBox</property>
                <child>
                  <object class="AdwActionRow" id="disablesandbox_row">
                    <property name="title">DisableSandBox</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="disablesandboxfs_row">
                    <property name="title">DisableSandBoxFilesystem</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="disablesandboxsys_row">
                    <property name="title">DisableSandBoxSyscalls</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">Packages</property>
                <child>
                  <object class="AdwActionRow" id="holdpkg_row">
                    <property name="title">HoldPkg</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="ignorepkg_row">
                    <property name="title">IgnorePkg</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="ignoregroup_row">
                    <property name="title">IgnoreGroup</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
