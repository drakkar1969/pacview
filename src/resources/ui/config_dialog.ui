                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="noupgrade_row">
                    <property name="title">NoUpgrade</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="noextract_row">
                    <property name="title">NoExtract</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">Miscellaneous</property>
                <child>
                  <object class="AdwActionRow" id="usesyslog_row">
                    <property name="title">UseSyslog</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="color_row">
                    <property name="title">Color</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="checkspace_row">
                    <property name="title">CheckSpace</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="cleanmethod_row">
                    <property name="title">CleanMethod</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="verbosepkglists_row">
                    <property name="title">VerbosePkgLists</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="ilovecandy_row">
                    <property name="title">ILoveCandy</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title">Signature Levels</property>
                <child>
                  <object class="AdwActionRow" id="siglevel_row">
                    <property name="title">SigLevel</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="localfilesiglevel_row">
                    <property name="title">LocalFileSigLevel</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="remotefilesiglevel_row">
                    <property name="title">RemoteFileSigLevel</property>
                    <property name="subtitle-selectable">true</property>
                    <style>
                      <class name="property"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
