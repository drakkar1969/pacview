<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// PACKAGE COLUMNVIEW //-->
  <template class="PkgColumnView" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkScrolledWindow">
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <property name="child">
          <object class="GtkColumnView" id="view">
            <property name="model">
              <object class="GtkSingleSelection" id="selection">
                <property name="model">
                  <object class="GtkSortListModel" id="sort_model">
                    <property name="model">
                      <object class="GtkFilterListModel" id="filter_model">
                        <property name="filter">
                          <object class="GtkEveryFilter" id="main_filter">
                            <child>
                              <object class="GtkCustomFilter" id="repo_filter"/>
                            </child>
                            <child>
                              <object class="GtkCustomFilter" id="status_filter"/>
                            </child>
                            <child>
                              <object class="GtkCustomFilter" id="search_filter"/>
                            </child>
                          </object>
                        </property>
                        <property name="model">
                          <object class="GListStore" id="model">
                            <property name="item-type">PkgObject</property>
                          </object>
                        </property>
                      </object>
                    </property>
                    <binding name="sorter">
                      <lookup name="sorter">view</lookup>
                    </binding>
                  </object>
                </property>
              </object>
            </property>
            <child>
              <object class="GtkColumnViewColumn" id="name_column">
                <property name="title">Package</property>
                <property name="resizable">True</property>
                <property name="expand">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="name_factory">
                    <property name="resource">/com/github/PacView/ui/column-name-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkStringSorter" id="name_sorter">
                    <property name="expression">
                      <lookup name="name" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="version_column">
                <property name="title">Version</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="version_factory">
                    <property name="resource">/com/github/PacView/ui/column-version-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkCustomSorter" id="version_sorter"/>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="repository_column">
                <property name="title">Repository</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="repository_factory">
                    <property name="resource">/com/github/PacView/ui/column-repository-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkStringSorter" id="repository_sorter">
                    <property name="expression">
                      <lookup name="repository" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="status_column">
                <property name="title">Status</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="status_factory">
                    <property name="resource">/com/github/PacView/ui/column-status-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkStringSorter" id="status_sorter">
                    <property name="expression">
                      <lookup name="status" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="date_column">
                <property name="title">Date</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="date_factory">
                    <property name="resource">/com/github/PacView/ui/column-date-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkNumericSorter" id="date_sorter">
                    <property name="expression">
                      <lookup name="install_date_raw" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="size_column">
                <property name="title">Size</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="size_factory">
                    <property name="resource">/com/github/PacView/ui/column-size-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkNumericSorter" id="size_sorter">
                    <property name="expression">
                      <lookup name="install_size_raw" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="group_column">
                <property name="title">Group</property>
                <property name="resizable">True</property>
                <property name="factory">
                  <object class="GtkBuilderListItemFactory" id="group_factory">
                    <property name="resource">/com/github/PacView/ui/column-group-item.ui</property>
                  </object>
                </property>
                <property name="sorter">
                  <object class="GtkStringSorter" id="group_sorter">
                    <property name="expression">
                      <lookup name="group" type="PkgObject"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
