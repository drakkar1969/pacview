<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// PACKAGE DETAILS WINDOW //-->
  <template class="PkgDetailsWindow" parent="AdwWindow">
    <property name="default_width">600</property>
    <property name="default_height">700</property>
    <property name="modal">True</property>
    <property name="title"></property>
    <property name="content">
      <object class="GtkBox" id="window_box">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="header_bar">
            <style>
              <class name="transparent"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="main_box">
            <property name="orientation">vertical</property>
            <property name="margin_start">24</property>
            <property name="margin_end">24</property>
            <property name="margin_bottom">24</property>
            <property name="spacing">24</property>
            <child>
              <object class="GtkBox" id="banner_box">
                <property name="orientation">vertical</property>
                <property name="margin_start">16</property>
                <property name="margin_end">16</property>
                <property name="margin_bottom">16</property>
                <property name="spacing">16</property>
                <child>
                  <object class="GtkImage" id="pkg_image">
                    <property name="halign">center</property>
                    <property name="icon-name">package-x-generic-symbolic</property>
                    <property name="icon-size">large</property>
                    <style>
                      <class name="bubble"/>
                      <class name="large"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="pkg_label">
                    <style>
                      <class name="title-2"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="button_box">
                <property name="spacing">12</property>
                <child>
                  <object class="GtkToggleButton" id="file_button">
                    <property name="hexpand">True</property>
                    <property name="active">True</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="margin_start">16</property>
                        <property name="margin_end">16</property>
                        <property name="margin_top">6</property>
                        <property name="margin_bottom">6</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkImage">
                            <property name="icon-name">text-x-generic-symbolic</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Files</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkToggleButton" id="tree_button">
                    <property name="hexpand">True</property>
                    <property name="group">file_button</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="margin_start">16</property>
                        <property name="margin_end">16</property>
                        <property name="margin_top">6</property>
                        <property name="margin_bottom">6</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkImage">
                            <property name="icon-name">list-remove-all-symbolic</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Tree</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkToggleButton" id="log_button">
                    <property name="hexpand">True</property>
                    <property name="group">file_button</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="margin_start">16</property>
                        <property name="margin_end">16</property>
                        <property name="margin_top">6</property>
                        <property name="margin_bottom">6</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkImage">
                            <property name="icon-name">org.gnome.Settings-file-history-symbolic</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Log</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkStack" id="content_stack">
                <child>
                  <object class="GtkStackPage">
                    <property name="name">files</property>
                    <property name="child">
                      <object class="GtkBox" id="file_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">16</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">text-x-generic-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="file_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="files_view">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <property name="model">
                                      <object class="GListStore" id="files_model">
                                        <property name="item-type">GtkStringObject</property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory" id="files_factory">
                                    <property name="resource">/com/github/PacView/ui/details-file-item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">tree</property>
                    <property name="child">
                      <object class="GtkBox" id="tree_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">16</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">list-remove-all-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="tree_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Dependency Tree</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkLabel" id="tree_label">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="xalign">0</property>
                                <property name="yalign">0</property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">log</property>
                    <property name="child">
                      <object class="GtkBox" id="log_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">16</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">org.gnome.Settings-file-history-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="log_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Log Messages</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="log_view">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <property name="model">
                                      <object class="GListStore" id="log_model">
                                        <property name="item-type">GtkStringObject</property>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory" id="log_factory">
                                    <property name="resource">/com/github/PacView/ui/details-log-item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
