<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// PACKAGE DETAILS WINDOW //-->
  <template class="PkgDetailsWindow" parent="AdwWindow">
    <property name="default_width">550</property>
    <property name="default_height">650</property>
    <property name="modal">True</property>
    <property name="title"></property>
    <child>
      <object class="GtkEventControllerKey">
        <signal name="key-pressed" handler="on_key_pressed" swapped="no"/>
      </object>
    </child>
    <property name="content">
      <object class="GtkBox" id="window_box">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="header_bar">
            <style>
              <class name="flat"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="main_box">
            <property name="orientation">vertical</property>
            <property name="margin_start">24</property>
            <property name="margin_end">24</property>
            <property name="margin_bottom">24</property>
            <property name="spacing">16</property>
            <child>
              <object class="GtkBox" id="banner_box">
                <property name="orientation">vertical</property>
                <property name="margin_start">16</property>
                <property name="margin_end">16</property>
                <property name="margin_bottom">24</property>
                <property name="spacing">16</property>
                <child>
                  <object class="GtkImage" id="pkg_image">
                    <property name="halign">center</property>
                    <property name="icon-name">package-x-generic-symbolic</property>
                    <property name="icon-size">large</property>
                    <style>
                      <class name="bubble"/>
                      <class name="large"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="pkg_label">
                    <style>
                      <class name="title-2"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="button_box">
                <property name="homogeneous">True</property>
                <property name="spacing">6</property>
                <child>
                  <object class="StackToggleButton" id="file_button">
                    <property name="hexpand">True</property>
                    <property name="active">True</property>
                    <property name="icon">details-files-symbolic</property>
                    <property name="text">Files</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                  </object>
                </child>
                <child>
                  <object class="StackToggleButton" id="tree_button">
                    <property name="hexpand">True</property>
                    <property name="group">file_button</property>
                    <property name="icon">details-tree-symbolic</property>
                    <property name="text">Tree</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                  </object>
                </child>
                <child>
                  <object class="StackToggleButton" id="log_button">
                    <property name="hexpand">True</property>
                    <property name="group">file_button</property>
                    <property name="icon">details-log-symbolic</property>
                    <property name="text">Log</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                  </object>
                </child>
                <child>
                  <object class="StackToggleButton" id="cache_button">
                    <property name="hexpand">True</property>
                    <property name="group">file_button</property>
                    <property name="icon">details-cache-symbolic</property>
                    <property name="text">Cache</property>
                    <signal name="toggled" handler="on_button_toggled" swapped="no"/>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkStack" id="content_stack">
                <child>
                  <object class="GtkStackPage">
                    <property name="name">files</property>
                    <property name="child">
                      <object class="GtkBox" id="file_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">details-files-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="file_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="files_view">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <property name="model">
                                      <object class="GtkStringList" id="files_model"/>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory" id="files_factory">
                                    <property name="resource">/com/github/PacView/ui/details/file-item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">tree</property>
                    <property name="child">
                      <object class="GtkBox" id="tree_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">details-tree-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="tree_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Dependency Tree</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkLabel" id="tree_label">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="xalign">0</property>
                                <property name="yalign">0</property>
                                <attributes>
                                  <attribute name="font-desc" value="Source Code Pro 11"/>
                                </attributes>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">log</property>
                    <property name="child">
                      <object class="GtkBox" id="log_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">details-log-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="log_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Log Messages</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="log_view">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <property name="model">
                                      <object class="GtkStringList" id="log_model"/>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory" id="log_factory">
                                    <property name="resource">/com/github/PacView/ui/details/log-item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">cache</property>
                    <property name="child">
                      <object class="GtkBox" id="cache_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">16</property>
                        <style>
                          <class name="card"/>
                        </style>
                        <child>
                          <object class="GtkBox">
                            <property name="margin_start">16</property>
                            <property name="margin_end">16</property>
                            <property name="margin_top">16</property>
                            <property name="spacing">12</property>
                            <child>
                              <object class="GtkImage">
                                <property name="icon-name">details-cache-symbolic</property>
                                <style>
                                  <class name="bubble"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="cache_header_label">
                                <property name="xalign">0</property>
                                <property name="yalign">0.5</property>
                                <property name="label">Cache</property>
                                <style>
                                  <class name="title-3"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="hexpand">True</property>
                            <property name="vexpand">True</property>
                            <property name="margin_bottom">16</property>
                            <property name="child">
                              <object class="GtkListView" id="cache_view">
                                <property name="margin_start">16</property>
                                <property name="margin_end">16</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <property name="model">
                                      <object class="GtkStringList" id="cache_model"/>
                                    </property>
                                  </object>
                                </property>
                                <property name="factory">
                                  <object class="GtkBuilderListItemFactory" id="cache_factory">
                                    <property name="resource">/com/github/PacView/ui/details/cache-item.ui</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
